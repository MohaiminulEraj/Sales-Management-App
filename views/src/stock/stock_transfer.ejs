<%- include('../partials/header.ejs') %>

<%- include('../partials/navbar.ejs') %>

<div class="container">
    <div class="row mb-3 mt-4">
        <div class="col">
            <label for="product_catId">Transfer Form </label>
            <select class="form-control" name="product_cat" id="product_catId" required>
                <option value="" selected disabled>Select</option>
                <option value="General">General</option>
                <option value="Health">Health</option>
                <option value="Computer">Computer</option>
                <option value="Home">Home</option>
                <option value="Toys">Toys</option>
                <option value="Automotive">Automotive</option>
                <option value="Music">Music</option>
                <option value="Tools">Tools</option>
            </select>
        </div>
    </div>
    <div class="row mb-3">
        <div class="col">
            <label for="products">Products </label>
            <input type="text" class="form-control" name="" id="products">
        </div>
    </div>
    <div class="row mb-3">
        <div class="col">
            <label for="productsQty">Products Qty </label>
            <input type="text" class="form-control" name="" id="productsQty">
        </div>
    </div>
    <div class="row mb-3">
        <div class="col">
            <label for="product_cat2Id">Transfer To </label>
            <select class="form-control" name="product_cat" id="product_cat2Id" required>
                <option value="" selected disabled>Select</option>
                <option value="General">General</option>
                <option value="Health">Health</option>
                <option value="Computer">Computer</option>
                <option value="Home">Home</option>
                <option value="Toys">Toys</option>
                <option value="Automotive">Automotive</option>
                <option value="Music">Music</option>
                <option value="Tools">Tools</option>
            </select>
        </div>
    </div>
    <button style="float: right;" id="stockTransfer" type="submit" class="btn btn-success mb-3">Stock Transfer</button>
</div>

<script src="../../assets/js/jquery-3.5.1.min.js"></script>
<%- include('../partials/footer.ejs') %>